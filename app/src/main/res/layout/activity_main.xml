<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.juniver.myapplication.MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button"
        android:layout_marginTop="76dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/button2"
        android:onClick="cambiarMensaje"
        android:longClickable="false"
        android:clickable="false" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/button2"
        android:layout_above="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:onClick="cambiarMensaje2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/button3"
        android:layout_alignTop="@+id/button2"
        android:layout_alignStart="@+id/button6"
        android:onClick="cambiarMensaje3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/button4"
        android:layout_below="@+id/button"
        android:layout_alignStart="@+id/button"
        android:onClick="cambiarMensaje4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/button5"
        android:layout_alignTop="@+id/button4"
        android:layout_alignStart="@+id/button8"
        android:onClick="cambiarMensaje5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/button6"
        android:layout_above="@+id/button9"
        android:layout_alignStart="@+id/button9"
        android:onClick="cambiarMensaje6" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/button7"
        android:layout_below="@+id/button4"
        android:layout_toStartOf="@+id/button8"
        android:onClick="cambiarMensaje7" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/button8"
        android:layout_alignTop="@+id/button7"
        android:layout_centerHorizontal="true"
        android:onClick="cambiarMensaje8" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/button9"
        android:layout_alignTop="@+id/button8"
        android:layout_toEndOf="@+id/button8"
        android:onClick="cambiarMensaje9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView1"
        android:textStyle="bold"
        android:textSize="25sp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reiniciar el Juego"
        android:id="@+id/button10"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp" />

</RelativeLayout>
